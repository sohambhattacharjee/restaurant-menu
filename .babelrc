{
	"presets": [
		"react",
		[ "env", {
			"targets": {
				"browsers": "last 2 versions",
				"node": "current"
			}
		}]
	],
	"plugins": [
		"transform-class-properties",
		"transform-object-rest-spread"
	],
	"env": {
		"test": {
			"presets": [
				[
					"env",
					{
						"modules": "commonjs"
					}
				]
			],
			"plugins": [
				"transform-class-properties",
				"transform-object-rest-spread"
			]
		}
	}
}
